<%= form_for @user, html: { id: "contactForm" } do |f| %>
  <fieldset>
    <%= f.label :jmeno %>
    <%= f.text_field :jmeno %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>

    <br>
    <%= f.submit 'Vytvořit uživatele!' %>

  </fieldset>
<% end %>
